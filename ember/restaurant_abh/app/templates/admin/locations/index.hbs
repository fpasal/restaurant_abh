{{#admin-navigation-bar addOption=true navColumn=10 page='locations'}}{{/admin-navigation-bar}}

<div class="container" style="margin-top:30px;">
  <div class="row">
    <div class="col-md-12">
      <table class="table table-bordered table-hover box_shadow administration_table">
        <thead>
          <tr>
            <td class="col-md-11">{{input type="checkbox" checked=rememberMe}} Name</td>
            <td class="col-md-1"></td>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>{{input type="checkbox" checked=rememberMe}} Atlanta</td>
            <td class="options">
              <span {{action 'edit' 2}} class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
              <span {{action 'delete' 2}} class="glyphicon glyphicon-trash" aria-hidden="true"></span>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>

