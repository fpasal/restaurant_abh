<div class="container" style="overflow:auto;">
  {{#each listRestaurants as |restaurantlist|}}
    <div class="restaurant_box">
      <img class="restaurant_thumb" src="assets/images/restaurants/thumbnails/{{restaurantlist.imageFileName}}.jpg" />
      {{#link-to 'restaurant' restaurantlist.id class='restaurant_name'}}{{restaurantlist.restaurantName}}{{/link-to}}
      <div class="restaurant_marks">
        <img src="assets/images/star_active.png" />
        <img src="assets/images/star_active.png" />
        <img src="assets/images/star_active.png" />
        <img src="assets/images/star_active.png" />
        <img src="assets/images/star_inactive.png" />
        <span class="votes">(1745)</span>
        <span class="price_range">
          {{restaurantlist.priceRange}}
        </span>
      </div>
      <div class="restaurant_food">{{restaurantlist.foodType}}</div>
      <a class="restaurant_reserve" href="#">Reserve now</a>
    </div>
  {{else}}
    Sorry, nobody is here.
  {{/each}}
</div>

